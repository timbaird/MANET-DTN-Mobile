<?xml version="1.0" encoding="UTF-8"?>
<ContentPage    Title="Create Data"
                xmlns="http://xamarin.com/schemas/2014/forms" 
                xmlns:x="http://schemas.microsoft.com/winfx/2009/xaml" 
                x:Class="MANET_DTN_Mobile.Views.CreateDataView">
	<StackLayout>
        <Entry Keyboard="Text" Placeholder="Data Title" x:Name="entryTitle"/>
        <Editor Keyboard="Text" VerticalOptions="FillAndExpand" x:Name="editorData"/>
        <Button Text="Send Data" Clicked="Handle_Button_Store_Clicked" />
    </StackLayout>
</ContentPage>
